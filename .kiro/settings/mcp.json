{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {},
      "disabled": true,
      "autoApprove": []
    }
  },
  "sequential": {
    "command": "npx",
    "args": [
      "@modelcontextprotocol/server-everything@latest"
    ],
    "env": {
      "FASTMCP_LOG_LEVEL": "ERROR"
    },
    "disabled": false,
    "autoApprove": [
      "create_task",
      "update_task",
      "list_tasks",
      "complete_task"
    ]
  },
  "firebase": {
    "command": "npx",
    "args": [
      "@modelcontextprotocol/server-filesystem@latest"
    ],
    "env": {
      "FASTMCP_LOG_LEVEL": "ERROR"
    },
    "disabled": false,
    "autoApprove": [
      "read_file",
      "write_file",
      "list_directory",
      "create_directory"
    ]
  },
  "puppeteer": {
    "command": "npx",
    "args": [
      "@modelcontextprotocol/server-puppeteer@latest"
    ],
    "env": {
      "FASTMCP_LOG_LEVEL": "ERROR"
    },
    "disabled": false,
    "autoApprove": [
      "screenshot",
      "navigate",
      "click",
      "type",
      "puppeteer_navigate"
    ]
  },
  "context7": {
    "command": "npx",
    "args": [
      "@modelcontextprotocol/server-everything@latest"
    ],
    "env": {
      "FASTMCP_LOG_LEVEL": "ERROR"
    },
    "disabled": false,
    "autoApprove": [
      "analyze_context",
      "generate_summary",
      "extract_insights",
      "echo",
      "sampleLLM",
      "annotatedMessage"
    ]
  }
}